global:
  scrape_interval:     {{ scrape }}
  evaluation_interval: {{ evaluation }}

  external_labels:
      monitor: 'messagebird-project'
      
scrape_configs:
  - job_name: 'prometheus'
    static_configs:
      - targets: ['0.0.0.0:9090']
        labels:
          alias: prometheus

    static_configs:
      - targets: ['{{ elb }}:80']
        labels:
          alias: webapp-metrics
